
# port for the REST interface of GReg
port_server=5002

# list of known hosts running a greg daemon. MUST be a valid and running host
known_hosts=130.149.22.133

# network interface to use
network_interface=eth0

# path for the log files
log_path=/usr/local/gReg/log

# en/disable saving datasets to local database
db_backup=1

# name of the database
db_database=globalregistry

# name of the table
db_table=dataset

# database username
db_user=root

# database user password
db_pass=secret
